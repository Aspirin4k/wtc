    {
      "characters": {
        "right": {
          "url": "c_ushiromia_jessica_uj_crying_a2",
          "z-index": 2
        }
      },
      "text": {
        "content": "George nii-san got upset, so <purple>we all went outside to look for them.</purple>",
        "character": "Jessica",
        "style": "add"
      },
      "effects": {
        "transition": "gradient-left"
      }
    },
    {
      "background": {
        "url": "b_garden_r1c_rain"
