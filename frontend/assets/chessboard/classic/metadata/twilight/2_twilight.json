    },
    {
      "background": {
        "url": "b_blood"
      },
      "text": {
        "content": "Everyone headed to Krauss's and Natsuhi's rooms together, ...and in Natsuhi's room, they found Krauss and Natsuhi lying sprawled on the floor.",
        "character": "narrator",
        "style": "replace"
      },
      "effects": {
        "sound": "e_impact"
      }
    },
    {
      "background": {
        "url": "black"
      },
      "text": null,
      "effects": {
        "transition": "fade-in",
        "transition_speed": 1000,
        "auto_transition": 0
      }
    },
    {
      "background": {
        "url": "b_natsuhi_room_2e_rain"
      },
      "effects": {
        "transition": "fade-in",
        "auto_transition": 0
      }
    },
    {
      "characters": {
        "middle": {
          "url": "c_gohda_goh_anger_a1",
          "z-index": 1
        }
      },
      "text": {
        "content": "<purple>Madam's room was locked!</purple> It's just like the dining hall!",
        "character": "Gohda",
        "style": "replace"
      },
      "effects": {
        "transition": "fade-in"
      }
    },
    {
      "background": {
        "url": "b_natsuhi_room_2b_rain"
