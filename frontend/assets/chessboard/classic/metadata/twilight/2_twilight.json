    },
    {
      "characters": {
        "right": {
          "url": "c_shannon_sha_puzzled_a1",
          "z-index": 1
        }
      },
      "text": {
        "content": "After getting everyone's permission, <purple>I unlocked the door...</purple>",
        "character": "Shannon",
        "style": "replace"
      },
      "effects": {
        "transition": "gradient-left"
      }
    },
    {
      "characters": {
        "left": {
          "url": "c_kanon_kan_troubled_a1",
          "z-index": 2
        }
      },
      "text": {
        "content": "<purple>Krauss-sama and Natsuhi-sama lay sprawled on the floor inside the room.</purple>",
        "character": "Kanon",
        "style": "add"
      },
      "effects": {
        "transition": "gradient-right"
      }
    },
    {
      "background": {
        "url": "b_natsuhi_room_2a_rain"
      },
      "characters": {
        "left": null,
        "right": null
      },
